{
  "name": "sudhaus7/picture",
  "description": "(Sudhaus7) Picture",
  "version": "1.0.0",
  "type": "typo3-cms-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Frank Berger",
      "role": "Developer"
    },
    {
      "name": "Markus Hofmann",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2",
    "typo3/cms-core": "^9.5 || ^10.3"
  },
  "require-dev": {
    "typo3/cms-fluid-styled-content": "^10.3",
    "typo3/cms-tstemplate": "^10.3",
    "typo3/cms-backend": "^10.3",
    "typo3/cms-t3editor": "^10.3",
    "phpstan/phpstan": "^0.11",
    "typo3/cms-lowlevel": "^10.3",
    "codeception/codeception": "^3.1",
    "codeception/phpbuiltinserver": "^1.5",
    "helhum/typo3-console": "v6.0.0-alpha2"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "picture",
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/public"
    }
  },
  "autoload": {
    "psr-4": {
      "SUDHAUS7\\Picture\\": "Classes/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/public/typo3conf/ext/",
      "[ -L .Build/public/typo3conf/ext/picture ] || ln -snvf ../../../../. .Build/public/typo3conf/ext/picture"
    ]
  }
}
